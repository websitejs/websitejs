{
    "name": "WebsiteJS Demo Website",
    
    "paths": {
        "src": "./src",
        "dest": "./dist",

        "tasks": "./gulp-tasks",

        "scss": {
            "src": "/",
            "dest": "/css",
            "docs": "/docs/styles"
        },

        "scripts": {
            "fileName": "scripts",
            "src": "/js",
            "dest": "/js",
            "docs": "/docs/scripts"
        },

        "elements": {
            "src": "/elements"
        },

        "components": {
            "src": "/components"
        },

        "styleguide": {
            "src": "/styleguide",
            "dest": "/styleguide"
        }
    }
}